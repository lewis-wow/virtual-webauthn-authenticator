    "type-fest": "^5.0.1",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@repo/exception": "workspace:*",
    "@repo/utils": "workspace:*",
    "buffer": "^6.0.3",
    "cbor2": "^2.0.1",
    "effect": "^3.19.4",
    "ts-pattern": "^5.8.0",
    "typanion": "^3.14.0",
