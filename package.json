{
  "name": "virtual-webauthn-authenticator",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "vitest run --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:coverage": "vitest run --coverage --config vitest.integration.config.ts",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" --check-ignore-pragma",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\" --check-ignore-pragma",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.5.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "barrelsby": "^2.8.1",
    "ejs": "^3.1.10",
    "eslint": "^9.34.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "@tailwindcss/oxide",
      "bcrypt",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
